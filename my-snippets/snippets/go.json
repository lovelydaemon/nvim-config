{
  "if error != nil": {
    "prefix": "ern",
    "body": "err != nil $0",
    "description": "shortcut for if err != nil"
  },
  "for loop": {
    "prefix": "for",
    "body": "for ${1} := ${2}; ${1} < ${3}; ${1}++$0",
    "description": "for snippet"
  },
  "for range": {
    "prefix": "forr",
    "body": [
      "for ${1:i}, ${2:v} := range ${3:arr} {",
      "\t$0",
      "}"
    ],
    "description": "for range snippet"
  },
  "fmain": {
    "prefix": "fmain",
    "body": [
      "package main",
      "",
      "func main() {$0}"
    ],
    "description": "function main"
  },
  "http ResponseWriter *Request": {
    "prefix": "wr",
    "body": "${1:w} http.ResponseWriter, ${2:r} *http.Request$0",
    "description": "http handler declaration"
  }
}
